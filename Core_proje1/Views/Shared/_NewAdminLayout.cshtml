@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="tr">
@await Html.PartialAsync("/Views/Admin/PartialHead.cshtml")
<body>
<div class="container-scroller">
<!-- partial:../../partials/_sidebar.html -->
<nav class="sidebar sidebar-offcanvas" id="sidebar">
	<div class="sidebar-brand-wrapper d-none d-lg-flex align-items-center justify-content-center fixed-top">
		<a class="sidebar-brand-wrapper mr-15"><img src="/Template/resim/adminlogo.png" width="175" height="80" style="padding: 14px;" mr-15 alt="logo" /></a>
	</div>
	@await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")
</nav>
<!-- partial -->
<div class="container-fluid page-body-wrapper">
	<!-- partial:../../partials/_navbar.html -->
	@await Html.PartialAsync("/Views/Admin/PartialNavbar.cshtml")
	<!-- partial -->
<div class="main-panel">
	<div class="content-wrapper">
			@RenderBody()
	</div>
	@await RenderSectionAsync("scripts", required: false)
	<!-- content-wrapper ends -->
	<!-- partial:../../partials/_footer.html -->
	@await Html.PartialAsync("/Views/Admin/PartialFooter.cshtml")
	<!-- partial -->
</div>
<!-- main-panel ends -->
</div>
<!-- page-body-wrapper ends -->
</div>


